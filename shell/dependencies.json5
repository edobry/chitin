{
    "tools": {
        "jq": {
            "version": "1.6",
            "command": "jq --version | awk -F '-' '{ print $2 }'"
        },
        "yq": {
            "version": "4.11.2",
            "command": "yq --version 2>&1 | awk '{ print $4 }' | sed 's/v//'"
        },
        "pcregrep": {
            "version": "8.45",
            "command": "pcregrep --version | cut -d ' ' -f 3"
        }
    },
    "chainDeps": {
        "init": {
            "dependencies": ["jq"]
        }
    }
}
