{
    "projectDir": "",
    "modules": {
        "aws-auth": {
            "enabled": false,
            "googleUsername": "",
            "departmentRole": ""
        },
        "k8s-env": {
            "enabled": false
        }
    },
    "dependencies": {
        "jq": {
            "command": "jq --version | awk -F '-' '{ print $2 }'",
            "version": "1.6"
        },
        "yq": {
            "command": "yq --version 2>&1 | awk '{ print $3 }'",
            "version": "3.4.0"
        },
        "aws": {
            "command": "aws --version | awk '{ print $1 }' | sed 's/aws-cli\\///'",
            "version": "2.0.57"
        }
    }
}
